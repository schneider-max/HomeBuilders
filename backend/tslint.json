{
  "extends": "tslint:recommended",
  "rules": {
    "max-line-length": {
      "options": [120]
    },
    "member-ordering": false,
    "no-consecutive-blank-lines": false,
    "object-literal-sort-keys": false,
    "ordered-imports": true,
    "quotemark": [true, "double"],
    "variable-name": [true, "allow-leading-underscore"]
  }
}